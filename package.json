{
  "name": "my-website",
  "version": "1.0.0",
  "description": "My website built with react and next",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "next build && next export && touch out/.nojekyll && touch out/CNAME && echo \"matheusalves.dev\" >> out/CNAME && git add out/ && git commit -m \"Deploy to gh-pages\" && git subtree push --prefix out origin gh-pages"
