  "bugs": {
    "url": "https://github.com/mhsalves/my-website/issues"
  },
  "homepage": "https://github.com/mhsalves/my-website#readme",
  "dependencies": {
    "next": "8.0.3",
    "react": "16.8.5",
    "react-dom": "16.8.5"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
